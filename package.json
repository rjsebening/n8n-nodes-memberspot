{
  "name": "n8n-nodes-memberspot",
  "version": "1.0.2",
  "description": "n8n custom node for Memberspot API integration",
  "license": "MIT",
  "homepage": "https://github.com/rjsebening/n8n-nodes-memberspot#readme",
  "author": {
    "name": "JÃ¶rg Sebening",
    "email": "dev@joerg-sebening.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjsebening/n8n-nodes-memberspot.git"
  },
  "bugs": {
    "url": "https://github.com/rjsebening/n8n-nodes-memberspot/issues"
  },
  "engines": {
    "node": ">=20.15"
  },
  "scripts": {
    "build": "npx rimraf dist && tsc && npm run build:icons",
    "build:icons": "gulp build:icons || echo 'No icons to build'",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/MemberspotApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Memberspot/Memberspot.node.js"
    ]
  },
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "memberspot",
    "automation",
    "workflow"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "~8.32.0",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "exit": "^0.1.2",
    "gulp": "^4.0.2",
    "gulp-replace": "^1.1.4",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
