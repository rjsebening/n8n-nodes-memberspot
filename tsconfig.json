{
  "compilerOptions": {
    "target": "ES2019",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "outDir": "./dist",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,

  },
  "include": ["nodes/**/*", "nodes/**/*.json", "credentials/**/*", "index.ts", "package.json"],
  "exclude": ["dist", "node_modules", "**/*.test.ts"]
}
